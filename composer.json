{
    "name": "stripe/stripe-mirakl-connector",
    "description": "Stripe-Mirakl connector",
    "keywords": [
        "stripe",
        "payment processing",
        "api",
        "mirakl"
    ],
    "homepage": "https://stripe.com/",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "api-platform/api-pack": "^1.2",
        "nelmio/api-doc-bundle": "^3.4",
        "satooshi/php-coveralls": "^2.2",
        "sensio/framework-extra-bundle": "^5.4",
        "shivas/versioning-bundle": "^3.2",
        "stripe/stripe-php": "^7.0.2",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/http-client": "4.3.*",
        "symfony/mailer": "4.3.*",
        "symfony/messenger": "4.3.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/proxy-manager-bridge": "4.3.*",
        "symfony/security-bundle": "4.3.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "hautelook/alice-bundle": "^2.5",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.11.15",
        "phpstan/phpstan-doctrine": "^0.11.5",
        "phpstan/phpstan-symfony": "^0.11.6",
        "spaze/phpstan-stripe": "^0.0.3",
        "symfony/browser-kit": "4.3.*",
        "symfony/maker-bundle": "^1.13",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "platform": {
          "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
